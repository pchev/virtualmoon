    <SearchPaths>
      <IncludeFiles Value="..\sources"/>
      <OtherUnitFiles Value="..\sources;..\sources\basis;..\sources\components;..\sources\objects"/>
      <UnitOutputDirectory Value="lib\$(TargetOs)\$(BuildMode)\$(TargetCPU)"/>
    </SearchPaths>
    <Parsing>
      <Style Value="1"/>
    </Parsing>
    <CodeGeneration>
      <SmartLinkUnit Value="True"/>
      <Optimizations>
        <OptimizationLevel Value="3"/>
      </Optimizations>
    </CodeGeneration>
    <Linking>
      <Debugging>
        <GenerateDebugInfo Value="False"/>
      </Debugging>
    </Linking>
    <Other>
      <Verbosity>
        <ShoLineNum Value="True"/>
        <ShowCompProc Value="True"/>
        <ShowHintsForUnusedUnitsInMainSrc Value="True"/>
      </Verbosity>
    </Other>
  </CompilerOptions>
</CONFIG>
